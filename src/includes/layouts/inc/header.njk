{%- from "macros/list-menu/list-menu.njk" import listmenu -%}
<header>
  <h1>{{sitedata.header.name}}</h1>
  <h2>{{sitedata.header.site_title}}</h2>
  <div class="profile-thumb" style="background-image:url('{{sitedata.config.imgPath + sitedata.meta.site_thumb}}');"></div>
  <nav>
    {{listmenu(sitedata.header.contact,"contact")}}
    {{listmenu(sitedata.header.nav,"main")}}
    {{listmenu(sitedata.header.pdf,"pdf")}}
  </nav>
</header>