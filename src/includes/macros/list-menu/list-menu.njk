{%- macro listmenu(data,className) -%}
    <nav class="{{className}}">
        <ul>
            {%- for item in data -%}
                <li>
                    <a href="{{item.url}}" class="{{item.class}}">{{item.text}}</a>
                </li>
            {%- endfor -%}
        </ul>
    </nav>
{%- endmacro -%}